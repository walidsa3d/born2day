{
  "name": "born2day",
  "version": "0.4.0",
  "description": "A CLI tool that provides comprehensive birthday insights including zodiac signs, historical events, and famous people",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "born2day": "./dist/index.js"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "mkdir -p dist && cp src/* dist/",
    "dev": "node src/index.js",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "test": "jest"
  },
  "keywords": [
    "birthday",
    "cli",
    "zodiac",
    "astrology",
    "history"
  ],
  "author": "walidsa3d",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.9",
    "chalk": "^5.4.1",
    "commander": "^12.1.0",
    "lunarphase-js": "^2.0.3"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testEnvironment": "node"
  }
}
